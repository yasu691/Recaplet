{
  "expo": {
    "name": "Recaplet",
    "slug": "recaplet",
    "version": "1.0.0",
    "scheme": "recaplet",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.recaplet.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": "https://yasu691.github.io/Recaplet"
      },
      "apiUrl": "https://recaplet.netlify.app",
      "eas": {
        "projectId": "8b05432f-0194-4dab-b7c7-d3ec605211d2"
      }
    }
  }
}
